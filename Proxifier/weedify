#/usr/bin/bash

WEED_TCP_SOCKET="../testounet_tcp.socket"
WEED_UDP_SOCKET="../testounet_udp.socket"
#WEED_UDP_SOCKET="../testounet3.socket "
PROXIFIER_LIB_PATH="."
PROXIFIER_SOCKET="client.sock"

if [ -e $PROXIFIER_SOCKET ]
then
        rm $PROXIFIER_SOCKET
fi
WEED_TCP_SOCKET=$WEED_TCP_SOCKET WEED_UDP_SOCKET=$WEED_UDP_SOCKET LD_PRELOAD=$PROXIFIER_LIB_PATH/connect.so $@


